<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.fanda.zeng.arouterdemo.MainActivity">

    <Button
        android:id="@+id/btn_simple_without_params"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="简单不带参数的界面跳转"/>

    <Button
        android:id="@+id/btn_simple_without_params_for_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="简单不带参数带结果的界面跳转"/>

    <Button
        android:id="@+id/btn_with_params"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="带参数的界面跳转"/>

    <Button
        android:id="@+id/btn_find_fragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="找到fragment界面"/>

    <Button
        android:id="@+id/btn_webview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="webview 跳转"/>

    <Button
        android:id="@+id/btn_use_service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="获取并调用服务"/>



</LinearLayout>
